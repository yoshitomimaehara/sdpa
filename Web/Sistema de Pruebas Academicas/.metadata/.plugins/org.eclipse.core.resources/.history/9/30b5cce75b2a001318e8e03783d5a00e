var press=false;
$(document).ready(function(){
	
	$("#ingresar").click(function(){
		$('.error').css("background-color","white");
		$('.error').fadeIn("slow");
	});
	
	$('.error').click(function(){
		$(this).fadeOut("slow");
	});
});